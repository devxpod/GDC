    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.6",
    "node-jose": "^2.2.0"
  },
  "devDependencies": {
    "@jest/types": "^30.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwk-to-pem": "^2.0.4",
    "@types/node": "^24.0.0",
    "@types/node-jose": "^1.1.13",
    "@types/supertest": "^6.0.2",
    "jest": "^30.0.0",
    "node-mocks-http": "^1.16.3",
    "supertest": "^7.0.1",
    "ts-jest": "^30.0.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.9.3"
  }
