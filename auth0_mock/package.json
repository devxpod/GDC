{
  "name": "auth0-docker-mocker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm install && npm run build",
    "start": "node .",
    "cleanup": "rm -rf node_modules keys.json",
